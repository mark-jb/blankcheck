  <title>Back to the Future</title>
  <link>https://audioboom.com/posts/7692151</link>
  <enclosure url="https://pdst.fm/e/chtbl.com/track/4E942/audioboom.com/posts/7692151.mp3?modified=1601250457&amp;source=rss&amp;stitched=1" length="0" type="audio/mpeg" />
  <media:content url="https://pdst.fm/e/chtbl.com/track/4E942/audioboom.com/posts/7692151.mp3?modified=1601250457&amp;source=rss&amp;stitched=1" type="audio/mpeg" medium="audio" duration="10900" lang="en-US" />
  <itunes:image href="https://images.theabcdn.com/i/36987789.jpg" />
